{
  "name": "create metric report",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "create metric report!",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon-20.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      }
    }
  },
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage",
    "tabs",
    "http://*/",
    "https://*/",
    "<all_urls>",
    "downloads"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "icon-20.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
